{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "3.3.0",
  "action": {
    "default_icon": {
      "16": "icons/zwp16.png",
      "32": "icons/zwp32.png",
      "48": "icons/zwp48.png",
      "64": "icons/zwp64.png",
      "128": "icons/zwp128.png",
      "256": "icons/zwp256.png",
      "512": "icons/zwp512.png",
      "1024": "icons/zwp1024.png"
    },
    "default_popup": "popup.html",
    "default_title": "ZWP by 0leads"
  },
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "16": "icons/zwp16.png",
    "32": "icons/zwp32.png",
    "48": "icons/zwp48.png",
    "64": "icons/zwp64.png",
    "128": "icons/zwp128.png",
    "256": "icons/zwp256.png",
    "512": "icons/zwp512.png",
    "1024": "icons/zwp1024.png"
  },
  "author": "Satyajit@kotaielectronics.com",
  "content_scripts": [
    {
      "js": [
        "js/content_script.js"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://web.whatsapp.com/*"
    ]
  },
  "homepage_url": "https://github.com/marcosvrs/wtf",
  "options_ui": {
    "open_in_tab": false,
    "page": "options.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "cookies"
  ],
  "short_name": "ZWP",
  "storage": {
    "managed_schema": "schema.json"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "resources": [
        "js/wa-js.js"
      ]
    }
  ]
}